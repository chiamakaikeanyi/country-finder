{
  "name": "country-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@tanstack/react-query": "4.29.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.26",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "sass": "^1.62.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "cypress run --component && cypress run --headless",
    "eject": "react-scripts eject",
    "lint": "eslint --cache ./src",
    "lint:fix": "eslint 'src/**/**/*.ts{,x}' --fix",
    "lint:style": "stylelint src/**/**/*.scss",
    "lint:style-fix": "stylelint 'src/**/**/*.scss' --fix",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "cypress": "cypress run --component",
    "cypress:open": "cypress open --component",
    "cypress:integration": "cypress run --headless",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/**/*.{ts,tsx}": "yarn run lint:fix",
    "src/**/**/*.scss": "yarn run lint:style-fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^4.29.15",
    "@types/leaflet": "^1.9.8",
    "@types/react-helmet": "^6.1.6",
    "@types/react-leaflet": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cypress": "^12.11.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "leaflet": "^1.9.4",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "react-leaflet": "^4.2.1",
    "stylelint": "^15.6.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-order": "^6.0.3"
  }
}
